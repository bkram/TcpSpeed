{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "TcpSpeed build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${file}",
				"-o",
				// "${fileDirname}/${fileBasenameNoExtension}",
				"output/TcpSpeed",
				"-Wimplicit-function-declaration",
				"-DLINUX"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}